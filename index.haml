\---
layout: default
\---

.posts.tencol
  {% for post in site.posts %}
  .sixcol.last
    /%span {{ post.date | date_to_string }}
    /%img{ src: '/images/posts/{{post.url | replace: "/","" | replace: ".html",""}}/thumb.png', alt: "{{ post.title }}" }
    %a{href: "{{ post.url }}"}
      %img{ src: 'http://placehold.it/400x400', alt: "{{ post.title }}" }
    /%a{href: "{{ post.url }}"} {{ post.title }}
  {% endfor %}
